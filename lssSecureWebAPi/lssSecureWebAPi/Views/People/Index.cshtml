@model List<lssWebApi2.Models.AddressBook>
@{

    ViewBag.Title = "List";

}
<div ng-app="AppModule">
<div ng-controller="PeopleController">
    <h4>Address Book</h4>

    <h2>
        <span>{{userName}}</span>
        <input type="button" class="btn" value="logoff" ng-click="logout()" />
    </h2>
    @Html.ActionLink("Add Person", "AddressBook", "People", new { area = "" }, null)
    <table class="table table-striped table-striped table-bordered">
        <thead>
            <tr>
                <th>Action</th>
                <th>Id</th>
                <th>Name</th>
             </tr>
        </thead>
        <tbody>
            @foreach (var person in Model)
            { 
            <tr>
                <td>
                    @Html.ActionLink("Edit1", "Edit", "People", new { id = person.AddressId }, null)
                </td>

                   <td>
                    <span>@person.AddressId</span>
                </td>
                <td>
                    <span>@person.Name</span>
                </td>
            </tr>
            }
        </tbody>
    </table>
    <div><span>{{Message}}</span></div>
</div>
</div>
